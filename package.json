{
  "name": "deckhand-icons",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
		"deploy": "pnpm build && npx gh-pages -d dist",
    "generate:icons": "tsx generate-icons.ts",
    "lint": "pnpm dlx eslint . --ignore-pattern 'generate-icons.ts'",
    "format": "prettier --write \"**/*.{js,ts,tsx,vue,json,md,scss,css,html}\""
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.26.8",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vicons/material": "^0.13.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^9.32.0",
    "less": "^4.2.2",
    "naive-ui": "^2.41.0",
    "prettier": "3.5.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "~5.7.3",
    "vite": "^6.1.0",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.0"
